(score (vers 2.1)
    (opt Render.SpacingMethod 1)    //fixed
    (opt Render.SpacingValue 30)
    (opt Staff.DrawLeftBarline false)
    (instrument (staves 2)(musicData
        (clef G p1)(clef F4 p2)

        (n c5 s p1 (stem down)(beam 1 ++))
        (n a3 s p2 (stem up)(beam 1 =-))
        (n d3 e p2 (stem up)(beam 1 -))

    ))
)
